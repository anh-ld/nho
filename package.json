{
  "name": "nho",
  "description": "1KB Web Component abstraction",
  "version": "0.0.1",
  "keywords": [
    "nho",
    "web component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anh-ld/nho.git"
  },
  "author": {
    "name": "Anh Le",
    "email": "ledzanh@gmail.com"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.es.js",
      "default": "./dist/index.umd.js"
    },
    "./dist/": "./dist/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "*.md"
  ],
  "scripts": {
    "build": "vite build --mode=lib",
    "build:example": "vite build --mode=example",
    "check": "npx @biomejs/biome check --apply .",
    "clean": "rm -rf dist example/dist",
    "dev": "vite"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "vite": "^4.4.5"
  }
}
